<div class="row">
  <h2><%= @user.username %>'s Posts</h2>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th></th>
        <th></th>
        <th>Date posted</th>
      </tr>
    </thead>

    <tbody>
      <% @posts.each do |post| %>
        <tr>
          <td><%= post.title %></td>
          <td><%= link_to 'Read', post_path(post.id) %></td>
          <td></td>
          <td><%= post.created_at.to_date %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
